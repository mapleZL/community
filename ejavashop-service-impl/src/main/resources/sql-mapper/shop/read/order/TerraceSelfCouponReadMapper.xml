<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ejavashop.dao.shop.read.order.TerraceSelfCouponReadDao">
	<resultMap id="TerraceSelfCouponResult" type="com.ejavashop.entity.order.TerraceSelfCoupon">
			<result property="id" column="id" />
			<result property="tradeNo" column="trade_no" />
			<result property="couponSn" column="coupon_sn" />
			<result property="memberId" column="member_id" />
			<result property="status" column="status" />
			<result property="createTime" column="create_time" />
			<result property="updateTime" column="update_time" />
			<result property="couponId" column="coupon_id" />
			<result property="couponValue" column="coupon_value" />
	</resultMap>
	
	<select id="get" parameterType="Integer" resultMap="TerraceSelfCouponResult">
		select
		   *
		from `terrace_self_coupon`
		where `id` = #{id}
	</select>
	
	<select id="getTerraceSelfCouponByTradeNo" parameterType="String" resultMap="TerraceSelfCouponResult">
		select
		   *
		from `terrace_self_coupon`
		where `trade_no` = #{tradeNo}
	</select>
	
</mapper>