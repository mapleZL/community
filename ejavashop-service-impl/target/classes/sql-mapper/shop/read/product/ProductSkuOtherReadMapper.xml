<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ejavashop.dao.shop.read.product.ProductSkuOtherReadDao">
	<resultMap id="ProductSkuOtherResult" type="com.ejavashop.entity.product.ProductSkuOther">
			<result property="id" column="id" />
			<result property="productId" column="product_id" />
			<result property="skuOther" column="sku_other" />
			<result property="createtime" column="createtime" />
	</resultMap>
	
	<select id="get" parameterType="Integer" resultMap="ProductSkuOtherResult">
		select
		   *
		from `product_sku_other`
		where `id` = #{id}
	</select>
	
	<select id="queryProductSkuOtherByProductId" resultMap="ProductSkuOtherResult">
		select * from `product_sku_other` where `product_id`= #{productId}
	</select>
	
</mapper>